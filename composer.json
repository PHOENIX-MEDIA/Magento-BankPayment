{
    "name": "phoenix-media/magento-bankpayment",
    "description": "PHOENIX MEDIA - Bank Payment",
    "type": "magento-module",
    "authors": [
        {
            "name": "PHOENIX MEDIA",
            "email": "support@phoenix-media.eu"
        }
    ],
  "homepage": "https://github.com/PHOENIX-MEDIA/Magento-BankPayment",
  "require": {
    "magento-hackathon/magento-composer-installer": "*"
  },
  "extra": {
    "map": [
      ["app/code/community/Phoenix/BankPayment/*", "app/code/community/Phoenix/BankPayment/"],
      ["app/etc/modules/Phoenix_BankPayment.xml", "app/etc/modules/Phoenix_BankPayment.xml"],
      ["app/design/adminhtml/default/default/template/bankpayment/*", "app/design/adminhtml/default/default/template/bankpayment/"],
      ["app/design/frontend/base/default/template/bankpayment/*", "app/design/frontend/base/default/template/bankpayment/"],
      ["app/locale/da_DK/Phoenix_BankPayment.csv", "app/locale/da_DK/Phoenix_BankPayment.csv"],
      ["app/locale/de_DE/Phoenix_BankPayment.csv", "app/locale/de_DE/Phoenix_BankPayment.csv"],
      ["app/locale/el_GR/Phoenix_BankPayment.csv", "app/locale/el_GR/Phoenix_BankPayment.csv"],
      ["app/locale/en_US/Phoenix_BankPayment.csv", "app/locale/en_US/Phoenix_BankPayment.csv"],
      ["app/locale/es_ES/Phoenix_BankPayment.csv", "app/locale/es_ES/Phoenix_BankPayment.csv"],
      ["app/locale/fr_FR/Phoenix_BankPayment.csv", "app/locale/fr_FR/Phoenix_BankPayment.csv"],
      ["app/locale/it_IT/Phoenix_BankPayment.csv", "app/locale/it_IT/Phoenix_BankPayment.csv"],
      ["app/locale/ja_JP/Phoenix_BankPayment.csv", "app/locale/ja_JP/Phoenix_BankPayment.csv"],
      ["app/locale/nb_NO/Phoenix_BankPayment.csv", "app/locale/nb_NO/Phoenix_BankPayment.csv"],
      ["app/locale/nl_NL/Phoenix_BankPayment.csv", "app/locale/nl_NL/Phoenix_BankPayment.csv"],
      ["app/locale/pl_PL/Phoenix_BankPayment.csv", "app/locale/pl_PL/Phoenix_BankPayment.csv"],
      ["app/locale/pt_BR/Phoenix_BankPayment.csv", "app/locale/pt_BR/Phoenix_BankPayment.csv"],
      ["app/locale/pt_PT/Phoenix_BankPayment.csv", "app/locale/pt_PT/Phoenix_BankPayment.csv"],
      ["app/locale/tr_TR/Phoenix_BankPayment.csv", "app/locale/tr_TR/Phoenix_BankPayment.csv"]
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
